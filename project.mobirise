          "_name": "form1",
          "_sourceTheme": "floram4",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"form1\" group=\"Form\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\" checked>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle1\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText1\">\n        <header>Card</header>\n        <input type=\"checkbox\" title=\"Picture\" name=\"showPicture\" checked>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Signature\" name=\"showSignature\" checked>\n        <input type=\"checkbox\" title=\"Description\" name=\"showDesc\" condition=\"showSignature\">\n        <input type=\"color\" title=\"BG\" name=\"cardBg\" value=\"#455921\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n            <input type=\"image\" title=\"Image\" value=\"../_images/background1.jpg\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n\n            <div class=\"right-side col-12 col-lg-6\">\n                <div class=\"padd\">\n                    <h5 class=\"mbr-section-title mbr-fonts-style f\" mbr-if=\"showTitle1\" data-app-selector=\".f\">Escríbenos aquí:</h5>\n                    <p class=\"mbr-text desc mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText1\" data-app-selector=\".desc\">\n                        Lorem ipsum dolor sit amet, pri omnium verterem id, sit labore dicunt an, ea civibus.</p>\n                    <div mbr-form class=\"mbr-form\">\n                        <!--Formbuilder Form-->\n                        <form action=\"ezequielrodriguez1991@gmail.com\" name=\"Form Name\" method=\"POST\" class=\"mbr-form form-with-styler\" data-verified=\"ezequielrodriguez1991@gmail.com\">\n                            <div class=\"row\">\n                                <div hidden=\"hidden\" data-form-alert class=\"alert alert-success col-12\">Gracias por contactarnos, en la brevedad posible nos comunicaremos contigo.</div>\n                                <div hidden=\"hidden\" data-form-alert-danger class=\"alert alert-danger col-12\">Oops...!\n                                    some problem!</div>\n                            </div>\n                            <div class=\"dragArea row\">\n                                <div class=\"col form-group mb-3 \" data-for=\"email\">\n                                    <input type=\"email\" name=\"email\" placeholder=\"Tu Correo\" data-form-field=\"email\" mbr-theme-style=\"display-7\" class=\"form-control\" value>\n                                </div>\n                                <div class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\" data-for=\"textarea\">\n                                    <textarea name=\"textarea\" placeholder=\"Tu Mensaje\" data-form-field=\"textarea\" mbr-theme-style=\"display-7\" class=\"form-control\" style=\"margin-top: 0px; margin-bottom: 0px; height: 33px;\"></textarea>\n                                </div>\n                                <div class=\"col-auto\" mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\"><a type=\"submit\" class=\"btn btn-danger\" data-app-placeholder=\"Type Text\">Enviar</a></div>\n                            </div>\n                        </form>\n                        <!--Formbuilder Form-->\n                    </div>\n                </div>\n            </div>\n            <div class=\"left-side col-12 col-lg-6\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-box align-center\">\n                        <div class=\"pic\" mbr-if=\"showPicture\">\n                            <img class=\"picture\" src=\"../_images/portr3.jpg\" alt=\"Mobirise\">\n                        </div>\n                        <h5 class=\"mbr-section-title art mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".art\">Diseños florales</h5>\n                        <p class=\"mbr-text text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".text, .si\">\"Haz tu evento inolvidable con VerdeAura. Contáctanos hoy.\"</p>\n\n                        <div class=\"si\" mbr-if=\"showSignature\">\n                            <img class=\"sign\" src=\"@PROJECT_PATH@/assets/images/logotipo%20oficial%20verde%20aura_vs%20blanco-600x600.png\" alt=\"Mobirise\">\n                            <p class=\"mbr-text desc mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showDesc\" data-app-selector=\".desc\"><i>florist, Mary Byrd</i></p>\n                        </div>\n                    </div>\n                </div>